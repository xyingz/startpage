{
    "version": "1.1.2",
    "name": "x-start-page",
    "description": "xiaopangying start page",
    "author": "jeremyjone",
    "license": "MIT",
    "scripts": {
        "dev": "vite",
        "build": "vue-tsc --noEmit && vite build",
        "release": "standard-version",
        "serve": "vite preview",
        "prepare": "husky install",
        "lint": "eslint src/**/*.{js,vue,ts,jsx,tsx}",
        "lint:fix": "eslint src/**/*.{js,vue,ts,jsx,tsx} --fix",
        "lint:create": "eslint --init",
        "lint:all": "eslint --fix",
        "lint-staged": "lint-staged",
        "test:unit": "jest --passWithNoTests",
        "commit": "git-cz",
        "init:tools": "node ./scripts/init-tools.js",
        "init:aphorisms": "node ./scripts/init-aphorisms.js",
        "init:searchs": "node ./scripts/init-searchs.js"
    },
    "lint-staged": {
        "*.{vue,js,ts,jsx,tsx}": [
            "eslint --fix"
        ]
    },
    "dependencies": {
        "@quasar/extras": "^1.12.3",
        "axios": "^0.22.0",
        "lunar-typescript": "^1.2.17",
        "quasar": "^2.4.12",
        "vue": "^3.2.26",
        "vue-router": "4",
        "vuex": "^4.0.2"
    },
    "devDependencies": {
        "@babel/core": "^7.15.8",
        "@babel/preset-env": "^7.15.8",
        "@babel/preset-typescript": "^7.15.0",
        "@commitlint/cli": "^13.2.1",
        "@commitlint/config-conventional": "^13.2.0",
        "@quasar/vite-plugin": "^1.0.4",
        "@types/jest": "^27.4.0",
        "@types/node": "^16.10.3",
        "@typescript-eslint/eslint-plugin": "^5.9.1",
        "@typescript-eslint/parser": "^5.9.1",
        "@vitejs/plugin-vue": "^2.0.1",
        "@vue/babel-plugin-jsx": "^1.1.1",
        "@vue/compiler-sfc": "^3.2.26",
        "@vue/test-utils": "^2.0.0-rc.18",
        "babel-jest": "26.0.0",
        "commitizen": "^4.2.4",
        "cz-conventional-changelog": "^3.3.0",
        "cz-customizable": "^6.3.0",
        "eslint": "^7.32.0",
        "eslint-config-airbnb-base": "^14.2.1",
        "eslint-config-airbnb-typescript": "^16.1.0",
        "eslint-config-prettier": "^8.3.0",
        "eslint-import-resolver-alias": "^1.1.2",
        "eslint-import-resolver-typescript": "^2.5.0",
        "eslint-plugin-import": "^2.25.2",
        "eslint-plugin-jest": "^25.0.5",
        "eslint-plugin-prettier": "^4.0.0",
        "eslint-plugin-vue": "^7.19.1",
        "husky": "^7.0.0",
        "jest": "26.0.0",
        "lint-staged": "^11.2.3",
        "prettier": "^2.4.1",
        "sass": "1.32.0",
        "standard-version": "^9.3.2",
        "ts-jest": "^26.4.4",
        "typescript": "^4.5.4",
        "vite": "^2.7.10",
        "vite-plugin-eslint": "^1.3.0",
        "vue-jest": "^5.0.0-alpha.10",
        "vue-tsc": "^0.30.2"
    },
    "config": {
        "commitizen": {
            "path": "./node_modules/cz-customizable"
        }
    }
}
